                    },
                    {
                        "name": "powerPS",
                        "in": "query",
                        "type": "integer"
                    },
                    {
                        "name": "kilometer",
                        "in": "query",
                        "type": "integer"
                    },
                    {
                        "name": "fuelType",
                        "in": "query",
                        "type": "string"
                    },
                    {
                        "name": "notRepairedDamage",
                        "in": "query",
                        "type": "string"
                    }
                ],
                "tags": [
                    "Cars"
                ]
