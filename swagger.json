                "parameters": [
                    {
                        "name": "principal",
                        "in": "query",
                        "type": "integer"
                    },
                    {
                        "name": "term",
                        "in": "query",
                        "type": "integer"
                    },
                    {
                        "name": "interest",
                        "in": "query",
                        "type": "number"
                    }
                ],
                "tags": [
                    "Cars"
                ]
            }
        },
        "/price": {
            "get": {
                "responses": {
                    "200": {
                        "description": "Success"
                    }
                },
                "description": "Gives user a recommended price to sell the car",
