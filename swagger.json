                "parameters": [
                    {
                        "name": "brand",
                        "in": "query",
                        "type": "string"
                    },
                    {
                        "name": "model",
                        "in": "query",
                        "type": "string"
                    },
                    {
                        "name": "vehicleType",
                        "in": "query",
                        "type": "string"
                    },
                    {
                        "name": "yearOfRegistration",
                        "in": "query",
                        "type": "string"
                    },
                    {
                        "name": "gearbox",
                        "in": "query",
