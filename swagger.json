{
    "swagger": "2.0",
    "basePath": "/",
    "paths": {
        "/cars/{budget}/{brand}": {
            "parameters": [
                {
                    "name": "budget",
                    "in": "path",
                    "required": true,
                    "type": "integer"
                },
                {
                    "name": "brand",
                    "in": "path",
                    "required": true,
                    "type": "string"
                }
            ],
            "get": {
                "responses": {
                    "200": {
                        "description": "Success"
                    }
                },
                "description": "Gives user a recommended car [list] for a given budget",
                "operationId": "get_cars",
                "tags": [
                    "Cars"
                ]
            }
        },
        "/comparisons": {
            "get": {
                "responses": {
                    "200": {
                        "description": "Success"
                    }
                },
                "description": "Compares 2 brands on their reliability index and average repair cost",
                "operationId": "get_comparisons",
                "parameters": [
                    {
                        "name": "Brand_1",
                        "in": "query",
                        "type": "string"
                    },
                    {
                        "name": "Brand_2",
                        "in": "query",
                        "type": "string"
                    }
                ],
                "tags": [
                    "Cars"
                ]
            }
        },
        "/graphcomparisons": {
            "get": {
                "responses": {
                    "200": {
                        "description": "Success"
                    }
                },
                "description": "Compares 2 brands on reliability index and average repair cost and generates a graph",
                "operationId": "get_graphcomparisons",
                "parameters": [
                    {
                        "name": "Brand_1",
                        "in": "query",
                        "type": "string"
                    },
                    {
                        "name": "Brand_2",
                        "in": "query",
                        "type": "string"
                    }
                ],
                "tags": [
                    "Cars"
                ]
            }
        },
